{
    "entryFile": "OpenFlow/src/ee/OpenAPIProxy.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "controllerPathGlobs2": ["src/**/*Controller.ts"],
    "controllerPathGlobs": ["OpenFlow/src/ee/OpenAPIuser.ts"],
    "spec": {
      "outputDirectory": "OpenFlow/src/public",
      "specVersion": 3,
      "securityDefinitions": {
        "oidc": {
          "type": "openIdConnect",
          "openIdConnectUrl": "https://app.openiap.io/oidc/.well-known/openid-configuration"
        }

      }
    },
    "routes": {
      "routesDir": "OpenFlow/src/ee/build",
      "authenticationModule": "OpenFlow/src/ee/OpenAPIauthentication.ts"
    }
  }